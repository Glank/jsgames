[
  {
    "name": "all",
    "always_run": true,
    "deps": [
      "test:mtx",
      "test:collision"
    ]
  },
  {
    "name": "mtx",
    "rule": "js_test",
    "in": [
      "test/mtx.mjs",
      "src/mtx.mjs"
    ],
    "out": [
      "bin/test/mtx.pass"
    ]
  },
  {
    "name": "collision",
    "rule": "js_test",
    "in": [
      "test/collision.mjs",
      "src/collision.mjs"
    ],
    "out": [
      "bin/test/collision.pass"
    ]
  }
]
